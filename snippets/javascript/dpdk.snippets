snippet dpdk.view
	define(function () {
		return Backbone.ExtendedView.extend({
	          template: _.template("" +
	              "<div>Home.View</div>" +
	          ""),
	  
	          // HTML Rendering
	  
	          renderChange: function (previousFragment, previousFragmentLanguage, currentFragment, currentFragmentLanguage) {
	              return true;
	          },
	  
	          render: function (previousFragment, previousFragmentLanguage, currentFragment, currentFragmentLanguage) {
	              //this.modelAsJSON = this.model.toJSON();
	              //console.log("MAIN MENU this.modelAsJSON", this.modelAsJSON);
	              this.el.html(this.template);
	              this.renderCompleted();
	          }
	  	});
	});
