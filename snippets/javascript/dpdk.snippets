snippet seq
	var sequence = new Backbone.Sequence();
	sequence.add(${1})
	sequence.execute();
snippet site.view
	${1:viewName}: {
		"el": "$1-${2:element}",

		"fragmentsByLanguage": {
			"en": ["$1/*deeplink"]${5:,}
		}

		"cache": ${4:true}
	}${3:,}
snippet site.model
	"${1:modelName}": {
		"restPathByLanguage": {
			"en": "$1",
		},

		"cache": true
	},
